{% block toolbar %}
    {% set icon %}

    {% if collector.runId %}
        <a href="{{ url("xhprof.run", {run: collector.runId}) }}" title="{{ 'XHProf'|t }}">
            <img width="20" height="28" alt="Asset"
                 src="data:image/png;base64,{{ collector.icon }}"/>
        </a>
    {% else %}
        <span>
        <img width="20" height="28" alt="Asset"
             src="data:image/png;base64,"/>
        </span>
    {% endif %}

    {% endset %}
    {% set text %}
    {% if collector.runId %}
        <div class="sf-toolbar-info-piece">
            <b>{{ 'Run id'|t }}</b>
            <span>{{ collector.runId }}</span>
        </div>
        {% if collector.showSummaryData %}
            <div class="sf-toolbar-info-piece">
                <b>{{ 'Calls'|t }}</b>
                <span>{{ collector.calls }}</span>
            </div>
            <div class="sf-toolbar-info-piece">
                <b>{{ 'Incl. Wall Time'|t }}</b>
                <span>{{ collector.wt }}</span>
            </div>
        {% endif %}
    {% endif %}
    {% endset %}

    <div class="sf-toolbar-block">
        <div class="sf-toolbar-icon">{{ icon|default('') }}</div>

        {% if collector.runId %}
            <div class="sf-toolbar-info">{{ text|default('') }}</div>
        {% endif %}
    </div>
{% endblock %}
