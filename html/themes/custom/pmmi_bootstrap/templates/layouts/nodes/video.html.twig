<div class="video-node">
  <div class="top-banner clearfix">
    {{ field_render([content.image_header, 'img-header']) }}
  </div>

  <h2 class="node-title">
    {{ content.title }}
  </h2>

  {{ field_render([content.video, 'video']) }}

  {{ field_render([content.date, 'date']) }}

  {{ field_render([content.body, 'node-body']) }}

  {{ field_render([content.featured_speaker, 'featured-speaker']) }}

  {{ field_render([content.link, 'register-link']) }}
</div>
