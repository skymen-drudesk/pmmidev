<div class="video-node">
  <h2 class="node-title">
    {{ content.title }}
  </h2>
  <div class="video">
    {{ content.field_video }}
  </div>
  <div class="date">
    {{ content.field_date }}
  </div>
  {% set body_text_rendered %}
    {{ content.body }}
  {% endset %}
  {% if body_text_rendered|trim is not empty %}
    <div class="node-body">
      {{ body_text_rendered }}
    </div>
  {% endif %}
  <div class="featured-speaker">
    {{ content.field_featured_speaker }}
  </div>
  <div class="register-link">
    {{ content.field_link }}
  </div>
</div>
