<div class="company-node">
  <div class="container">
    <h2 class="node-title">
      {{ content.title }}
    </h2>

    <div class="general">
      {{ field_render([content.general_line_1, 'line-1']) }}
      {{ field_render([content.general_line_2, 'line-2']) }}
    </div>

    {{ field_render([content.content_row_blue, 'content-row-blue']) }}
    {{ field_render([content.content_row_1, 'content-row-1']) }}
    {{ field_render([content.content_row_2, 'content-row-2']) }}
    {{ field_render([content.trade_shows, 'trade-shows']) }}
    {{ field_render([content.updated, 'updated']) }}

  </div>
</div>
