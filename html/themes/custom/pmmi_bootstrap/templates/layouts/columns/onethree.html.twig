{#
/**
 * @file
 * Template for a 3-up (1 row of 3 columns) layout.
 */
#}
<div {{ content['#attrs'].addClass('layout-3up clearfix') }}>
  <div class="region-top">
    {{ content.top }}
  </div>
  <div class="rows container">
    <div class="row-1 row">
      <div class="col col-1 col-sm-4">
        {{ content.oneone }}
      </div>
      <div class="col col-2 col-sm-4">
        {{ content.onetwo }}
      </div>
      <div class="col col-3 col-sm-4">
        {{ content.onethree }}
      </div>
    </div>
  </div>
  <div class="region-bottom">
    {{ content.bottom }}
  </div>
</div>
