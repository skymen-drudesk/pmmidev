{#
/**
 * @file
 * Template for a 1-up (1 row of 1 column) layout.
 */
#}
<div class="layout-1up clearfix">
  <div class="region-top col-sm-12">
    {{ content.top }}
  </div>
  <div class="rows">
    <div class="row-1">
      <div class="col col-1 col-sm-12">
        {{ content.oneone }}
      </div>
    </div>
  </div>
  <div class="region-bottom col-sm-12">
    {{ content.top }}
  </div>
</div>
