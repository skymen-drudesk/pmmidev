<?php

/**
 * @file
 * The install file for the pmmi_personify_sso module.
 */

use Drupal\user\Entity\Role;

/**
 * Implements hook_install().
 */
function pmmi_personify_sso_install() {
  if (Role::load('personify_user') == NULL) {
    $role = Role::create(['id' => 'personify_user', 'label' => 'Personify user']);
    $role->save();
  }
}
