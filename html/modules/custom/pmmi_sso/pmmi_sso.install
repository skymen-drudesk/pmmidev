<?php

/**
 * @file
 * The install file for the pmmi_sso module.
 */

use Drupal\user\Entity\Role;

/**
 * Implements hook_install().
 */
function pmmi_sso_install() {
  if (Role::load('pmmi_member') == NULL) {
    $role = Role::create(['id' => 'pmmi_member', 'label' => 'PMMI Member']);
    $role->save();
  }
}
