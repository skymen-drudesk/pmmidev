  "conflict": {
    "drupal/drupal": "*"
  },
  "autoload": {
    "classmap": [
      "build/scripts/composer/ScriptHandler.php"
    ],
    "psr-4": {
      "PhpOffice\\PhpSpreadsheet\\": "vendor/phpoffice/phpspreadsheet/src/PhpSpreadsheet"
    }
  },
  "scripts": {
    "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
    "post-install-cmd": [
      "DrupalProject\\composer\\ScriptHandler::buildScaffold",
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ],
    "post-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::buildScaffold",
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ]
  },
  "config": {
    "preferred-install": "source"
  },
  "extra": {
    "installer-paths": {
      "html/core": [
        "type:drupal-core"
      ],
      "html/libraries/{$name}": [
        "type:drupal-library"
      ],
      "html/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "html/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "html/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "html/drush/commands/{$name}": [
        "type:drupal-drush"
      ]
    },
    "preserve-paths": [
      "html/sites/all/modules/contrib",
      "html/sites/all/modules/custom",
      "html/sites/all/modules/feature_modules",
      "html/sites/all/themes/contrib",
      "html/sites/all/libraries",
      "html/sites/default/settings.php",
      "html/sites/default/files"
    ],
    "merge-plugin": {
      "include": [
        "html/modules/custom/audience_select/composer.json",
        "html/modules/custom/pmmi_twitter/composer.json",
        "html/modules/custom/pmmi_facebook/composer.json"
      ],
      "recurse": false,
      "replace": false,
      "merge-extra": false
    },
    "enable-patching": true,
    "patches-file": "composer.patches.json"
  }
}
