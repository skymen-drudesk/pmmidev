{
    "name": "PMMI",
    "type": "project",
    "license": "propriatary",
    "authors": [
        {
            "name": "David Newcorn",
            "role": "Manager"
        },
        {
            "name": "Jen Krepelka",
            "role": "Designer"
        },
        {
            "name": "Doug Dobrzynski",
            "role": "Programmer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.drupal-composer.org"
        }
    ],
    "minimum-stability": "alpha",
    "prefer-stable": true,
    "require": {
        "composer/installers": "^1.0.20",
        "drupal-composer/drupal-scaffold": "^1.3.1",
        "cweagans/composer-patches": "~1.0",
        "derhasi/composer-preserve-paths": "0.1.*",
        "drupal/admin_menu": "8.3.*@dev",
        "drupal/bootstrap": "8.3.*@beta",
        "drupal/config_readonly": "8.1.*",
        "drupal/config_update": "8.1.*",
        "drupal/console": "~0.10",
        "drupal/core": "~8.0",
        "drupal/date": "8.1.*@dev",
        "drupal/diff": "8.1.*@dev",
        "drupal/eform": "8.1.*@dev",
        "drupal/features": "8.3.*",
        "drupal/job_scheduler": "8.2.*@dev",
        "drupal/page_manager": "8.1.*",
        "drupal/panels": "8.3.*",
        "drupal/pathauto": "8.1.*@dev",
        "drupal/token": "8.1.*@dev",
        "drush/drush": "~8.0"
    },
    "require-dev": {
        "behat/mink": "~1.6",
        "behat/mink-goutte-driver": "~1.2",
        "drupal/coder": "8.2.*",
        "drupal/config_devel": "8.1.*@beta",
        "drupal/devel": "8.1.*@dev",
        "drupal/diff": "8.1.*",
        "drupal/stage_file_proxy": "8.1.*@dev",
        "drupal/xhprof": "8.1.*@dev",
        "jcalderonzumba/gastonjs": "^1.1@dev",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "mikey179/vfsStream": "~1.2",
        "phpunit/phpunit": "~4.8",
        "symfony/css-selector": "2.7.*"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "autoload": {
        "classmap": [
            "build/scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::buildScaffold",
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::buildScaffold",
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "config": {
        "preferred-install": "source"
    },
    "extra": {
        "installer-paths": {
            "html/core": ["type:drupal-core"],
            "html/modules/contrib/{$name}": ["type:drupal-module"],
            "html/profiles/contrib/{$name}": ["type:drupal-profile"],
            "html/themes/contrib/{$name}": ["type:drupal-theme"],
            "html/drush/commands/{$name}": ["type:drupal-drush"]
        },
        "preserve-paths": [
            "html/sites/all/modules/contrib",
            "html/sites/all/modules/custom",
            "html/sites/all/modules/feature_modules",
            "html/sites/all/themes/contrib",
            "html/sites/all/libraries",
            "html/sites/default/settings.php",
            "html/sites/default/files"
        ],
        "patches-file": "composer.patches.json"
    }
}
