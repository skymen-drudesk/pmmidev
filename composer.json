{
    "name": "PMMI",
    "type": "project",
    "license": "propriatary",
    "authors": [
        {
            "name": "David Newcorn",
            "role": "Manager"
        },
        {
            "name": "Jen Krepelka",
            "role": "Designer"
        },
        {
            "name": "Doug Dobrzynski",
            "role": "Programmer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.drupal-composer.org"
        }
    ],
    "minimum-stability": "alpha",
    "prefer-stable": true,
    "require": {
        "composer/installers": "^1.0.20",
        "cweagans/composer-patches": "1.*",
        "drupal/admin_menu": "8.3.*@dev",
        "drupal/bootstrap": "8.3.*@beta",
        "drupal/config_readonly": "8.1.*",
        "drupal/config_update": "8.1.*",
        "drupal/console": "0.10.*",
        "drupal/core": "8.0.*",
        "drupal/date": "8.1.*@dev",
        "drupal/diff": "8.1.*@dev",
        "drupal/eform": "8.1.*@dev",
        "drupal/features": "8.3.*",
        "drupal/job_scheduler": "8.2.*@dev",
        "drupal/page_manager": "8.1.*",
        "drupal/panels": "8.3.*",
        "drupal/pathauto": "8.1.*@dev",
        "drupal/token": "8.1.*@dev",
        "drush/drush": "8.*"
    },
    "require-dev": {
        "drupal/coder": "8.2.*",
        "drupal/config_devel": "8.1.*@beta",
        "drupal/devel": "8.1.*@dev",
        "drupal/diff": "8.1.*",
        "drupal/stage_file_proxy": "8.1.*@dev",
        "drupal/xhprof": "8.1.*"
    },
    "scripts": {
        "post-install-cmd": [
            "bin/wrapper",
            "build/scripts/composer/post-install.sh"
        ]
    },
    "config": {
        "preferred-install": "source"
    },
    "extra": {
        "installer-paths": {
            "www/core": ["type:drupal-core"],
            "www/modules/contrib/{$name}": ["type:drupal-module"],
            "www/profiles/contrib/{$name}": ["type:drupal-profile"],
            "www/themes/contrib/{$name}": ["type:drupal-theme"],
            "www/drush/commands/{$name}": ["type:drupal-drush"]
        },
        "patches-file": "composer.patches.json"
    }
}
